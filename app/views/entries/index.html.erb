<% @entries.each do |entry| %>
  <% game = @games.find_by(id: entry.game_id) %>
  <%= form_for pool_entry_path(id: params[:pool_id], entry_id: entry.id), method: :post do |f|%>
    <%= f.label game.home_team %>
    <%= f.radio_button game, "yes" %>
    <%= f.label game.away_team %>
    <%= f.radio_button game, "yes" %>
  <% end %>
<% end %>
