    <% if !user_signed_in? %>
    <div class="wrapper align-items-center justify-content-center row">
      <div class="jumbotron text-center">
        <h1 class="display-3">Welcome to Poolster</h1>
        <h3 class="lead">Get your office pool off pen and paper already</h3>
        <%= button_to('Sign Up', new_user_registration_path, :class => 'btn btn-lg btn-success', method: :get) %>
      </div>
    </div>
    <% else %>
      <%= render 'dashboard' %>
    <% end %>
